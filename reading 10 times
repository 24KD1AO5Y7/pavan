#include <stdio.h>

int main() {
    char name[100];
    
    // Loop to read name 10 times
    for(int i = 0; i < 10; i++) {
        printf("Enter name #%d: ", i + 1);
        fgets(name, sizeof(name), stdin);  // Read name (including spaces)
        printf("You entered: %s\n", name);
    }
    
    return 0;
}
